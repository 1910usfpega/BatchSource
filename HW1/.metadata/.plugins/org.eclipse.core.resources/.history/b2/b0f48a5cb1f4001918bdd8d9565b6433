package com.revature.q9;

import java.util.ArrayList;

public class Prime {
	
	public static ArrayList<Integer> primes = new ArrayList<Integer>();
	
	public static void prime() {

		for (int i = 1; i <= 100; i++) { 		  	  
	        boolean flag = false;
	        for(int j = 2; j <= i/2; j++) {
	            if(i % j == 0)
	            {
	                flag = true;
	                break;
	            }
	        }
	        if (!flag) {
	        	primes.add(i);
	        }
	        else {
	        }
	    }
		System.out.println(primes.toString());
	}

}
